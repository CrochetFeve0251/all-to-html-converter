{
    "name": "crochetfeve0251/all-to-html-converter",
    "description": "A converter that convert multiple types of files to html",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "COQUARD Cyrille",
            "email": "coquardcyr@eisti.eu"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ncjoes/office-converter": "^1.0",
        "gufy/pdftohtml-php": "~2"
    },
    "autoload": {
        "psr-4": {
            "AllToHmlConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AllToHmlConverter\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "scripts": {
        "test": [
            "@php vendor/bin/phpunit"
        ]
    }
